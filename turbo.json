{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build:watch": {
      "outputs": [
        "./dist/**"
      ]
    },
    "build": {
      "outputs": [
        "./dist/**"
      ]
    },
    "lint": {},
    "start:dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    }
  }
}
